{
  "name": "@deneb-kaitos/sched-browser-front-end",
  "version": "0.0.0",
  "description": "[app] friendly calendar",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@7.23.0",
  "engines": {
    "node": "19.4.0",
    "pnpm": "7.23.0"
  },
  "engineStrict": true,
  "volta": {
    "node": "19.4.0"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "test-old": "playwright test",
    "test:unit": "vitest",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "dependencies": {
    "@js-temporal/polyfill": "^0.4.3",
    "inter-ui": "^3.19.3",
    "xstate": "^4.35.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.29.2",
    "@sveltejs/adapter-node": "1.1.0",
    "@sveltejs/kit": "^1.0.7",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.8.2",
    "prettier-plugin-svelte": "^2.9.0",
    "svelte": "^3.55.0",
    "svelte-check": "^3.0.1",
    "svelte-feathers": "^0.2.7",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vitest": "^0.26.3"
  },
  "dependenciesMeta": {
    "inter-ui": {
      "injected": true
    },
    "@js-temporal/polyfill": {
      "injected": true
    },
    "xstate": {
      "injected": true
    }
  }
}
